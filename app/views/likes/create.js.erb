var div = ".<%= dom_id(@likeable, :feed) %>";

$(div).replaceWith("<%= j render partial: @likeable %>");
// Need to check context of the like.  if in a modal-body-text want to replace with the modal form and remove the image
$(div).each(function(_,e){
  if ($(e).parent(".modal-body-text").length) {
    $(e).find("img.width100")[0].remove()
    $(e).find('.new_comment').replaceWith("<%= j render 'comments/form_modal', commentable: @likeable  %>")

  }
});
