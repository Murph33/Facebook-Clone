<h1>ALL USERS</h1>

<% @users.each do |user| %>
  <% unless user == current_user  %>
    <div class="">
      <% unless current_user.friends.include?(user) ||
        current_user.inverse_friends.include?(user) ||
        current_user.requestees.include?(user) %>
        <%= link_to "Friend",  user_requests_path(user), method: :post %>
      <% end %>
        <%= link_to "#{user.full_name}", user %>
    </div>
  <% end %>
<% end %>
